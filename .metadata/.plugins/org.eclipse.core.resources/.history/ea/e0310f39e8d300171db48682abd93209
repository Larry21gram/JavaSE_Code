package Morning;
	/*权限修饰符
	 * public 共有的
	 * private  私有的
	 * protected  受保护的（）同包能使用的
	 * 默认 默认情况的（同包）
	 * 
	 * 包 ： package 等同于文件夹
 	 * Java中即是 类所装的文件夹。
 	 * 命名： 公司网址的倒置 如 com.LanOu
	 * 
	 */
public class Test {
	public static void main(String[] args) {
		//这是在声明一个引用
		About_Method ab  ;
		// am 是一个
		About_Method am = new About_Method ();
		// 这是在传递引用
		ab = am ; 
		am.smile();
		
		
	}
}
