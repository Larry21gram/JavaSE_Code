package Morning;

public class About_Method {
	public static void main(String[] args) {
	
	}
	
	public void smile() {
		System.out.println("心里乐开花了吧！！！");
	}
	private void cry() {
		System.out.println("哭的像个孩子！！！");
	}
	protected void angry() {
		System.out.println("气的像条狗！！！");
	}
	String love() {
		return"爱是一道光！！！" ;
	}
}
