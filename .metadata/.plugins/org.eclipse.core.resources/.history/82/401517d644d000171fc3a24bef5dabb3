package Homework;

public class TestDemo {
	public static void main(String[] args) {
		//1 打印
		print();
		//2、 编程计算： 189元如何使用最少的100，50，20，10 ，5 ， 1组成。并输出各自的值。
		// 分别定义 100 .50 20 .10 .5 .1 的张数
		int a = 189 / 100 ;
		int b = 189 % 100 / 50 ;
		int c = 189 % 100 % 50 / 20 ;
		int d = 189 % 100 % 50 % 20 / 10 ;
		int e =  189 % 100 % 50 % 20 % 10 / 5 ;
		int f = 189 % 100 % 50 % 20 % 10% 5 / 1;
		int sum = a + b + c + d + e + f ;
		System.out.println(sum);
		
	}
	public static void print() {
		System.out.println("*****************");
		System.out.println(" ***  JAVA   ***");
		System.out.println("*****************");
	}
	public static int getCount(int num) {
		int a = 0 , b = 0 , c = 0 , d = 0 , e = 0 ,f = 0  ;
		int temp = 0; 
		a = num / 100 ;
		temp = num % 100 ;
		b = temp / 50 ;
		temp = temp % 50 ;
		c = temp / 20 ;
		temp = temp % 20 ;
		d = temp / 10 ;
		temp = temp % 10 ;
		e = temp / 5 ;
		temp = temp % 5 ;
		f = temp ;
		return a + b + c + d + e + f ;
	}
}
