package Homework;

public class TestDemo {
	public static void main(String[] args) {
		//1 打印
		print();
		//2、 编程计算： 189元如何使用最少的100，50，20，10 ，5 ， 1组成。并输出各自的值。
		// 分别定义 100 .50 20 .10 .5 .1 的张数
		int a = 189 % 100 ;
		int b = a % 50 ;
		int c = b % 20 ;
		int d = c % 10 ;
		int e = d % 5 ;
		int f = e % 1;
		int sum = a + b + c + d + e + f ;
		System.out.println(sum);
		
	}
	public static void print() {
		System.out.println("*****************");
		System.out.println(" ***  JAVA   ***");
		System.out.println("*****************");
	}
}
